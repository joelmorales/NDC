		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="AirlineID"/>
			<xsd:element name="Name" type="ProperNameSimpleType">
				<xsd:annotation>
					<xsd:documentation source="description" xml:lang="en">Airline Name.  Example: British Airways</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="FlightNumber"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="OperatingCarrierFlightType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for OPERATING CARRIER Supplier Role with Optional Flight Number.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="AirlineID">
				<xsd:annotation>
					<xsd:documentation>A two-character alpha-numeric code assigned according to the IATA Resolution 762 or a three-letter airline designator assigned by ICAO.  Refer to IATA Airline Coding Directory.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Name" type="ProperNameSimpleType">
				<xsd:annotation>
					<xsd:documentation source="description" xml:lang="en">Airline Name.  Example: British Airways</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="FlightNumber"/>
		</xsd:sequence>
	</xsd:complexType>
