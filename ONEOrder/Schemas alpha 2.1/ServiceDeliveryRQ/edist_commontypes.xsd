	<xsd:complexType name="SimpleCurrencyPriceType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">SIMPLE CURRENCY PRICE BASIS UNIT: NO price break-down, taxes and/or fees.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="CurrencyAmountOptType"/>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="PercentageValue" type="WholePercentageSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">Percentage Value relating to acceptable the total amount by which the Order item price can vary; OR the amount of a penalty. 

Example: 20 USD, 2 Percent</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
