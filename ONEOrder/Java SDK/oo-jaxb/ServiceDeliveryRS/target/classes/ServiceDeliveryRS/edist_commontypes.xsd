	<xsd:complexType name="TravelerSummaryType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for SUMMARY Traveler representation.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Name">
				<xsd:annotation>
					<xsd:documentation source="description" xml:lang="en">Passenger Name information.

Note: Detailed Name information, such as prefix, title and suffix, are specified in PassengerMetadata.</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Surname" type="ProperNameSimpleType">
							<xsd:annotation>
								<xsd:documentation source="description" xml:lang="en">Family name, last name

Example: SMITH
</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="Given" type="ProperNameSimpleType" minOccurs="0" maxOccurs="5">
							<xsd:annotation>
								<xsd:documentation source="description" xml:lang="en">Given name, first name or names</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="Title" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Name Title. Examples: MR, MRS, DR</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="SurnameSuffix" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Surname Suffixes and / or Letters, Examples:Jr., Sr., III., Ret., Esq., Ph.D., M.D.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="Middle" type="xsd:string" minOccurs="0" maxOccurs="3">
							<xsd:annotation>
								<xsd:documentation source="description" xml:lang="en">Middle name or initial.  Examples: G, GEORGE</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="ContactMethod" type="xsd:string" minOccurs="0"/>
			<xsd:element name="PassengerAssociation" type="xsd:string" minOccurs="0"/>
			<xsd:element name="PassengerTypeCode" type="xsd:string"/>
			<xsd:element name="ResidenceCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation source="description" xml:lang="en">Residence country code.
Examples: US, FR

Encoding Scheme: ISO 3166-1 (two character) Country Code</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="CountrySimpleType"/>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Age" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation source="description" xml:lang="en">Age and/or birthdate.</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:choice>
						<xsd:element name="Value">
							<xsd:annotation>
								<xsd:documentation source="description" xml:lang="en">Traveler age value.
Example: 38</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:simpleContent>
									<xsd:extension base="xsd:positiveInteger">
										<xsd:attribute name="UOM" type="AgeUnitSimpleType" use="optional">
											<xsd:annotation>
												<xsd:documentation source="description" xml:lang="en">Age unit of measure.
Example: Year</xsd:documentation>
											</xsd:annotation>
										</xsd:attribute>
									</xsd:extension>
								</xsd:simpleContent>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="BirthDate">
							<xsd:annotation>
								<xsd:documentation source="description" xml:lang="en">Traveler birth date.  Example: YYYY-MM-DD</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:simpleContent>
									<xsd:extension base="xsd:date"/>
								</xsd:simpleContent>
							</xsd:complexType>
						</xsd:element>
					</xsd:choice>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="BirthCountry" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Country of Birth. Country Name or Code. For Example. Germany, DE</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CitizenshipCountryCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Citizenship country code.
Examples: US, FR

Encoding Scheme: ISO 3166-1 (two character) Country Code</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="CountrySimpleType"/>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>
			<xsd:element ref="AddressContact" minOccurs="0"/>
		</xsd:sequence>
